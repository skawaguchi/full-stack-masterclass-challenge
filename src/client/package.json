{
    "name": "client",
    "version": "1.0.0",
    "private": true,
    "dependencies": {
        "@fortawesome/fontawesome": "1.1.0",
        "@fortawesome/fontawesome-free-solid": "5.0.2",
        "@fortawesome/react-fontawesome": "0.0.17",
        "axios": "0.17.1",
        "case-sensitive-paths-webpack-plugin": "2.1.1",
        "chalk": "1.1.3",
        "css-loader": "0.28.7",
        "dotenv": "4.0.0",
        "extract-text-webpack-plugin": "3.0.2",
        "file-loader": "1.1.5",
        "fs-extra": "3.0.1",
        "history": "4.7.2",
        "html-webpack-plugin": "2.29.0",
        "mobx": "3.4.1",
        "mobx-react": "4.3.5",
        "object-assign": "4.1.1",
        "postcss-flexbugs-fixes": "3.2.0",
        "postcss-loader": "2.0.8",
        "promise": "8.0.1",
        "prop-types": "15.6.0",
        "raf": "3.4.0",
        "react": "^16.2.0",
        "react-dev-utils": "^4.2.1",
        "react-dom": "^16.2.0",
        "react-router-dom": "4.2.2",
        "react-test-renderer": "16.2.0",
        "style-loader": "0.19.0",
        "sw-precache-webpack-plugin": "0.11.4",
        "url-loader": "0.6.2",
        "webpack": "3.8.1",
        "webpack-dev-server": "2.9.4",
        "webpack-manifest-plugin": "1.3.2",
        "whatwg-fetch": "2.0.3"
    },
    "devDependencies": {
        "autoprefixer": "7.1.6",
        "babel-core": "6.26.0",
        "babel-eslint": "8.1.2",
        "babel-jest": "20.0.3",
        "babel-loader": "7.1.2",
        "babel-plugin-transform-decorators-legacy": "1.3.4",
        "babel-preset-react-app": "^3.1.0",
        "babel-runtime": "6.26.0",
        "chance": "1.0.13",
        "enzyme": "3.3.0",
        "enzyme-adapter-react-16": "1.1.1",
        "eslint": "4.10.0",
        "eslint-config-airbnb-base": "12.0.2",
        "eslint-config-react-app": "^2.0.1",
        "eslint-loader": "1.9.0",
        "eslint-plugin-flowtype": "2.39.1",
        "eslint-plugin-import": "2.7.0",
        "eslint-plugin-jest": "21.2.0",
        "eslint-plugin-jsx-a11y": "5.1.1",
        "eslint-plugin-react": "7.4.0",
        "jest": "20.0.4",
        "sinon": "4.1.3",
        "stylelint": "8.4.0",
        "stylelint-config-standard": "18.0.0"
    },
    "scripts": {
        "build": "node scripts/build.js",
        "coverage": "node scripts/test.js --env=jsdom --coverage",
        "eslint": "npx eslint ./{src,__tests__}/**/*.js --cache",
        "lint": "yarn eslint && yarn stylelint",
        "start": "node scripts/start.js",
        "stylelint": "npx stylelint 'src/**/*.css'",
        "test": "node scripts/test.js --env=jsdom",
        "verify": "yarn lint && yarn coverage"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,mjs}",
            "!src/{mockUtils,index,registerServiceWorker}.{js,jsx,mjs}"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 100,
                "functions": 100,
                "lines": 100,
                "statements": 100
            }
        },
        "setupFiles": [
            "<rootDir>/config/polyfills.js"
        ],
        "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
            "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.js",
            "mjs",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ]
    },
    "babel": {
        "plugins": [
            "transform-decorators-legacy"
        ],
        "presets": [
            "react-app"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
